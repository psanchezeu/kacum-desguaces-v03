{
  "services": [
    {
      "name": "frontend",
      "type": "node",
      "source": ".",
      "env": {
        "VITE_API_URL": "http://backend:3001"
      }
    },
    {
      "name": "backend",
      "type": "node",
      "source": "./backend",
      "env": {
        "PORT": "3001",
        "DATABASE_URL": "file:./prisma/dev.db",
        "NODE_ENV": "production"
      }
    }
  ]
}
