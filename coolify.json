{
  "services": [
    {
      "name": "frontend",
      "type": "node",
      "source": ".",
      "build": {
        "cmd": "npm run build"
      },
      "start": {
        "cmd": "npm run dev"
      },
      "env": {
        "VITE_API_URL": "http://localhost:3001"
      }
    },
    {
      "name": "backend",
      "type": "docker",
      "source": "backend",
      "ports": [
        3001
      ],
      "env": {
        "PORT": "3001",
        "DATABASE_URL": "file:../prisma/dev.db"
      }
    }
  ],
  "databases": []
}
